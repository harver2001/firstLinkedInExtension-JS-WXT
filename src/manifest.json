{
    "name": "LinkedIn Chat Logo Injector",
    "version": "1.0",
    "description": "Injects a custom logo into every active LinkedIn chat window.",
    "manifest_version": 3,
    "permissions": [
      "tabs",
      "activeTab",
      "https://www.linkedin.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content-scripts/content.js"]
      }
    ]
  }
  